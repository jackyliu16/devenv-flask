            "iconColour": "#FFFF00",
            "foreground": "#FFFF00"
        },
        "FIXME": {
            "background": "#FF0000",
            "icon": "beaker",
            "rulerColour": "#FF0000",
            "iconColour": "#FF0000",
        },
        "BUG": {
            "icon": "bug",
            "foreground": "#FF0000",
        },
        "HACK": {
            "icon": "tools",
            "foreground": "#d2723a",
            "background": "#ffffff",
        },
        "XXX": {
            "icon": "x",
        },
        "OPTIMIZE": {
            "foreground": "#FFA500",
            "background": "#ffffff",
        },
        "CHECK": {
            "foreground": "#0080ffee",
            "background": "#ffffff",
        },
