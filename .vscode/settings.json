    //todo-tree settings
    "todo-tree.regex.regex": "((//|#|<!--|;|/\\*|^)\\s*($TAGS):|^\\s*- \\[ \\])",
    "todo-tree.general.tags": [
        "TODO",        // 有功能代码待编写
        "FIXME",       // 标识代码需要修正，甚至错误 
        "NOTE",        // 代码的注释或备注 
        "BUG",         // 常用于软件开发中指代程序错误或缺陷(由测试提交)
        "HACK",        // 临时措施，表明代码中存在的不太优雅、但在特定情况下有效的解决方案 
        "OPTIMIZE",    // 表明代码中存在的可以进一步优化的部分。这些代码片段可能已经能够正常工作，但是可能会在大规模数据或用户量上变得缓慢或不可扩展
        "XXX",         // 表明代码中存在的需要注意或待处理的问题。这些问题可能是技术债务、设计缺陷、功能不足等
        "CHECK",       // 表明代码中存在需要检查或验证的部分。这些部分可能需要进一步的测试、审查或评估，以确保代码的正确性和可靠性
        "[ ]", "[x]",  // 表示一个未/已完成的任务或待处理事项
        "tag", "done", // 指示代码中的特定功能或组件
    ],
    "todo-tree.regex.regexCaseSensitive": false,
    "todo-tree.highlights.defaultHighlight": {
        // "foreground": "#FFFFFF",
        // "background": "#FFFF00",
        // "icon": "check",
        // "rulerColour": "#FFFF00",
        // "type": "tag",
        // "iconColour": "#FFFF00"
    },
    "todo-tree.highlights.customHighlight": {
        "TODO": {
            // "background": "#FFFF00",
